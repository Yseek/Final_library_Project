<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="toolguys.library.library.mapper.user.MybooklistMapper">
    <select id="getMybooklist" resultType="Book" parameterType="long">
        select * from BOOK where BOOKSEQ in (select BOOK_BOOKSEQ from MYBOOKS where MEMBER_MEMBERSEQ=#{memberSeq})
    </select>
</mapper>